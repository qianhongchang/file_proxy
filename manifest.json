{
    "manifest_version": 2,
    "name": "奇代（代理工具）",
    "description": "代理工具-指哪代哪",
    "version": "1.1",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "tabs",
      "<all_urls>"
    ],
    "browser_action": {
      "default_icon": "./icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["./popup.js"],
      "persistent": true
    }
  }